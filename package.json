{
  "name": "coloringbook",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.2.1",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.19.0",
    "lodash": "^4.17.15",
    "polished": "^3.4.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-github-btn": "^1.1.1",
    "react-helmet": "^5.2.1",
    "react-markdown": "^4.2.2",
    "react-spring": "^8.0.27",
    "styled-components": "^4.4.1"
  },
  "scripts": {
    "webapp:start": "react-scripts start",
    "webapp:build": "react-scripts build",
    "webapp:docs": "npm run webapp:docs:build && serve ./typedoc-dist && start https://localhost:5000",
    "webapp:docs:build": "typedoc --out docs --ignoreCompilerErrors --out ./typedoc-dist --name \"Coloringbook 0.0.2\" --theme \"minimal\" --readme ./docs/webapp.md ./src/webapp",
    "ui": "npm run ui:start",
    "ui:start": "npm run test:build && start-storybook -p 9009 -s public",
    "ui:build": "npm run test:build && build-storybook -s public -o storybook-dist",
    "ui:deploy": "npm run ui:build && netlify --deploy --dir=storybook-dist --prod",
    "ui:deploy:skip-build": "netlify deploy --dir=storybook-dist --prod",
    "test": "npm run test:start",
    "test:start": "jest --verbose --passWithNoTests",
    "test:build": "jest --silent --passWithNoTests --bail=100 --json --outputFile=./public/jest-results.json || (exit 0)",
    "test:reset-all-snapshots": "jest -u",
    "start": "npm run webapp:start",
    "build": "npm run webapp:build"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^5.3.0-rc.0",
    "@storybook/addon-actions": "^5.3.0-rc.0",
    "@storybook/addon-docs": "^5.3.0-rc.0",
    "@storybook/addon-jest": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-storysource": "^5.2.8",
    "@storybook/addon-viewport": "^5.3.0-rc.0",
    "@storybook/addons": "^5.3.0-rc.0",
    "@storybook/preset-create-react-app": "^1.5.0",
    "@storybook/react": "^5.3.0-rc.0",
    "@testing-library/react": "^9.4.0",
    "@types/jest": "^24.0.24",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^5.0.14",
    "@types/react-test-renderer": "^16.9.1",
    "@types/styled-components": "^4.4.1",
    "axios-mock-adapter": "^1.17.0",
    "cross-env": "^6.0.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-config-react-app": "^0.5.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-loader": "1.6.0",
    "eslint-plugin-compat": "^3.0.1",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.6.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-optimize-regex": "^1.1.6",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-app": "^5.0.1",
    "eslint-plugin-react-hooks": "^2.1.1",
    "eslint-plugin-sonarjs": "^0.4.0",
    "husky": "^3.1.0",
    "jest-styled-components": "^6.3.4",
    "lint-staged": "^9.5.0",
    "netlify-cli": "^2.25.0",
    "prettier": "^1.19.1",
    "react-scripts": "3.3.0",
    "react-test-renderer": "^16.12.0",
    "serve": "^11.3.0",
    "stylelint": "^12.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "stylelint-processor-styled-components": "^1.9.0",
    "ts-jest": "^24.2.0",
    "typedoc": "^0.15.6",
    "typescript": "~3.7.2"
  }
}
